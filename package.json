{
  "name": "trainstagram",
  "version": "1.0.0",
  "description": "a feed based social media featuring trains, exclusively",
  "main": "backend/src/app.ts",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "mkdir -p ./dist && cp .env ./dist/.env",
    "start": "tsc && node ./backend/src/app.ts",
    "start:dev": "npx nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willacharlotte/trainstagram.git"
  },
  "author": "Willa Charlotte Lyle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/willacharlotte/trainstagram/issues"
  },
  "homepage": "https://github.com/willacharlotte/trainstagram#readme",
  "nodemonConfig": {
    "watch": [
      "./backend/",
      "./frontend/"
    ],
    "ext": "*",
    "delay": 2000
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mssql": "^9.1.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.32.1",
    "tedious": "^16.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/ms": "^0.7.31",
    "@types/mssql": "^8.1.2",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/node": "^20.4.4",
    "nodemon": "^3.0.1",
    "sequelize-typescript": "^2.1.5"
  }
}
